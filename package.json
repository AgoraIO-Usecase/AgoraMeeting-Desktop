  "scripts": {
    "preinstall": "node ./scripts/checkYarn.js",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "release:apaas:sdk": "lerna exec --scope=agora-meeting-sdk npm run pack:npm",
    "release:classroom:sdk": "run-s release:apaas:sdk",
    "release:electron:mac": "run-s release:demo pack:electron:mac",
    "release:electron:win": "run-s release:demo pack:electron:win",
    "release:core": "run-s build:rte-sdk build:meeting-core",
    "release:demo": "run-s build:apaas",
    "dev": "run-s run:demo",
    "dev:apaas": "run-s run:demo",
    "dev:electron": "run-s build:dev:rte-sdk build:dev:meeting-core build:dev:plugin build:dev:widget run:demo:electron",
    "build:apaas": "run-s build:demo",
    "pack:sdk": "run-s build:rte-sdk build:classroom:sdk",
    "build:classroom:sdk": "lerna exec --scope=agora-meeting-sdk npm run pack:npm",
    "build:meeting:sdk": "lerna exec --scope=agora-meeting-sdk npm run pack:npm",
    "build:dev:rte-sdk": "lerna exec --scope=agora-rte-sdk npm run build:dev",
    "build:rte-sdk": "lerna exec --scope=agora-rte-sdk npm run build",
    "build:dev:core": "lerna exec --scope=agora-meeting-core npm run build:dev:js",
    "build:core": "lerna exec --scope=agora-meeting-core npm run build:js",
    "build:demo": "lerna exec --scope=agora-meeting-sdk npm run build",
    "run:build:rte-sdk": "run-s build:rte-sdk",
    "run:demo": "lerna exec --scope=agora-meeting-sdk npm run dev",
    "run:demo:electron": "lerna exec --scope=agora-meeting-sdk npm run electron",
    "run:apaas": "lerna exec --scope=agora-apaas-demo npm run dev",
    "run:ui-kit": "lerna exec --scope=agora-meeting-ui yarn storybook",
    "test": "lerna run test",
    "clean": "lerna clean",
    "ci:build:web": "yarn release:demo",
    "ci:build:electron:win": "yarn release:electron:win",
    "ci:build:electron:mac": "yarn release:electron:mac",
    "ci:build:classroom:sdk": "yarn release:classroom:sdk"
  },
